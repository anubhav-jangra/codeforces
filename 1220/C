int32_t main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    //freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);
    string s;
    cin >> s;
    int n=s.length();
    int a[n];
    a[0]=s[0]-'a';
    for(int i=1;i<n;i++)
    {
        a[i]=min(a[i-1],(int)(s[i]-'a'));
    }
    for(int i=0;i<n;i++)
    {
        if(s[i]-'a'==a[i])
            cout << "Mike" << endl;
        else
            cout << "Ann" << endl;
    }
	return 0;
}
