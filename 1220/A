int32_t main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    //freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);
    int n;
    cin >> n;
    int a=0,b=0,c=0,d=0,e=0;
    for(int i=0;i<n;i++)
    {
        char dp;
        cin >> dp;
        if(dp=='z')
            a++;
        if(dp=='e')
            b++;
        if(dp=='r')
            c++;
        if(dp=='o')
            d++;
        if(dp=='n')
            e++;
    }
    for(int i=0;i<e;i++)
        cout << 1 << " ";
    for(int i=0;i<a;i++)
        cout << 0 << " ";
	return 0;
}
